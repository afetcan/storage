{
  "name": "@afetcan/storage",
  "private": false,
  "type": "module",
  "version": "0.0.17",
  "description": "Storage for afetcan.com",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "homepage": "https://github.com/afetcan/#readme",
  "packageManager": "pnpm@7.26.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afetcan/storage.git"
  },
  "bugs": {
    "url": "https://github.com/afetcan/storage/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./createMikroORMPostgress": {
      "types": "./dist/createMikroORMPostgress.d.ts",
      "import": "./dist/createMikroORMPostgress.js"
    }
  },
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.26.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.0.0-dev.34",
    "@mikro-orm/migrations": "^6.0.0-dev.34",
    "@mikro-orm/postgresql": "^6.0.0-dev.34",
    "@mikro-orm/seeder": "^6.0.0-dev.34",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@swc/core": "^1.3.32",
    "@types/uuid": "^9.0.0",
    "diary": "^0.4.3",
    "dotenv": "^16.0.3",
    "geojson": "^0.5.0",
    "graphql": "16.6.0",
    "knex": "^2.4.2",
    "nanoid": "^4.0.1",
    "pg": "^8.9.0",
    "slugify": "^1.6.5",
    "unique-username-generator": "^1.1.3",
    "uuid": "^9.0.0",
    "wellknown": "^0.5.0",
    "wkt-parser-helper": "^4.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@faker-js/faker": "^7.6.0",
    "@types/geojson": "^7946.0.10",
    "@types/pg": "^8.6.6",
    "@types/wellknown": "^0.5.4",
    "@vitest/coverage-c8": "^0.28.4",
    "bumpp": "^8.2.1",
    "consola": "^2.15.3",
    "eslint": "^8.33.0",
    "tslib": "^2.5.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
